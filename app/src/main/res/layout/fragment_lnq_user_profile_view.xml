            <TextView
                android:id="@+id/textViewconnections"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4sdp"
                android:gravity="start"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_16ssp"
                app:layout_constraintEnd_toStartOf="@+id/recyclerViewGroupNames"
                app:layout_constraintStart_toStartOf="@+id/textViewName"
                app:layout_constraintTop_toBottomOf="@+id/textViewCompany" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/slideView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_bottomsheet_corners"
            android:elevation="@dimen/_10sdp"
            android:padding="@dimen/_10sdp"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/mTvAccountHeading1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_30sdp"
                android:text="Awais Sarwar"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <Button
                android:id="@+id/relativeLayoutSendMessage"
                style="@style/buttonNewTheme"
                android:layout_width="@dimen/_220sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/messages"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mTvAccountHeading1">

            </Button>


            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:background="@color/colorBlackSettingItemsNewTheme"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/relativeLayoutSendMessage">

            </View>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/filtersRoot"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                app:layout_constraintStart_toStartOf="@+id/view"
                app:layout_constraintTop_toBottomOf="@+id/view">

                <TextView
                    android:id="@+id/textViewSortBy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:text="@string/settings"
                    android:textAllCaps="false"
                    android:textColor="@color/colorBlackSettingItemsNewTheme"
                    android:textSize="@dimen/_16ssp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textViewFavorites"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:text="@string/favorite"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_16ssp"
                    app:layout_constraintEnd_toStartOf="@+id/switchCompatFavroiut"
                    app:layout_constraintStart_toStartOf="@+id/textViewSortBy"
                    app:layout_constraintTop_toBottomOf="@+id/textViewSortBy" />

                <TextView
                    android:id="@+id/textViewVerifiedProfiles"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:text="@string/location_hidden"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_16ssp"
                    app:layout_constraintEnd_toStartOf="@+id/switchCompatLocationHidden"
                    app:layout_constraintStart_toStartOf="@+id/textViewSortBy"
                    app:layout_constraintTop_toBottomOf="@+id/textViewFavorites" />

                <TextView
                    android:id="@+id/textViewPendingLnq"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:text="@string/fully_block"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_16ssp"
                    app:layout_constraintEnd_toStartOf="@+id/switchCompatFullyBlock"
                    app:layout_constraintStart_toStartOf="@+id/textViewSortBy"
                    app:layout_constraintTop_toBottomOf="@+id/textViewVerifiedProfiles" />

                <Button
                    android:id="@+id/textViewUnLnq"
                    style="@style/buttonWhiteNewTheme"
                    android:layout_width="@dimen/_220sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:text="@string/un_lnq"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/switchCompatFullyBlock" />

                <Button
                    android:id="@+id/textViewProfileExportUser"
                    style="@style/buttonWhiteNewTheme"
                    android:layout_width="@dimen/_220sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:text="@string/export"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textViewUnLnq" />

                <Button
                    android:id="@+id/textViewAddToGroup"
                    style="@style/buttonWhiteNewTheme"
                    android:layout_width="@dimen/_220sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:text="@string/add_to_group"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textViewProfileExportUser" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchCompatFavroiut"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewFavorites"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/textViewFavorites" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchCompatLocationHidden"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewVerifiedProfiles"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/textViewVerifiedProfiles" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchCompatFullyBlock"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewPendingLnq"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/textViewPendingLnq" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_4sdp"
                android:layout_marginTop="@dimen/_25sdp"
                android:background="@color/colorGrayNewTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/filtersRoot">

            </View>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
            android:id="@+id/viewScroll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_50sdp"
            app:layout_constraintBottom_toBottomOf="@+id/tobBar"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tobBar">

        </View>

        <View
            android:id="@+id/viewHideTopBar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80sdp"
            android:visibility="gone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/slideView">

        </View>


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
