<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mRoot"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhiteNewTheme"
        android:clickable="true"
        android:focusable="true"
        tools:context="lnq.com.lnq.fragments.fullprofileview.FragmentLnqUserProfileView">

        <include
            android:id="@+id/tobBar"
            layout="@layout/cus_topbar_contacts_new_theme"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_70sdp"
            android:background="@mipmap/bg_map">

            <io.supercharge.shimmerlayout.ShimmerLayout
                android:id="@+id/shimmerLayoutFullProfile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:shimmer_animation_duration="2000">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <include layout="@layout/cus_shimmer_fullprofile" />

                </LinearLayout>

            </io.supercharge.shimmerlayout.ShimmerLayout>

            <TextView
                android:id="@+id/textViewName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:singleLine="true"
                android:textColor="@color/colorBlackSettingItemsNewTheme"
                android:textSize="@dimen/_16ssp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textViewJobTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_11ssp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewCompany"
                app:layout_constraintStart_toStartOf="@+id/textViewName"
                app:layout_constraintTop_toTopOf="@+id/textViewCompany" />

            <TextView
                android:id="@+id/textViewCompany"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:singleLine="true"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_11ssp"
                app:layout_constraintEnd_toStartOf="@+id/recyclerViewGroupNames"
                app:layout_constraintStart_toEndOf="@+id/textViewJobTitle"
                app:layout_constraintTop_toBottomOf="@+id/textViewName" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewProfileVerifiedTick"
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/textViewName"
                app:layout_constraintEnd_toStartOf="@+id/textViewName"
                app:layout_constraintStart_toEndOf="@+id/buttonLnq"
                app:layout_constraintTop_toTopOf="@+id/textViewName"
                app:srcCompat="@mipmap/profile_tic_new" />

            <Button
                android:id="@+id/buttonLnq"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_25sdp"
                android:layout_marginEnd="@dimen/_15sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:background="@mipmap/btn_lnq_new"
                android:gravity="center"
                android:padding="@dimen/_1sdp"
                android:text="LNQ'd"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/_9ssp"
                android:textStyle="bold"
                android:visibility="gone"
                android:textAllCaps="false"
                app:layout_constraintBottom_toBottomOf="@+id/textViewName"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textViewName" />

            <!--        <androidx.appcompat.widget.AppCompatImageView-->
            <!--            android:id="@+id/imageViewMenu"-->
            <!--            android:layout_width="@dimen/_22sdp"-->
            <!--            android:layout_height="@dimen/_22sdp"-->
            <!--            android:layout_marginEnd="@dimen/_20sdp"-->
            <!--            android:layout_marginRight="@dimen/_20sdp"-->
            <!--            app:layout_constraintBottom_toBottomOf="@+id/textViewName"-->
            <!--            app:layout_constraintEnd_toEndOf="parent"-->
            <!--            app:layout_constraintTop_toTopOf="@+id/textViewName"-->
            <!--            app:srcCompat="@drawable/icon_menu_more"-->
            <!--            app:tint="@color/colorWhite" />-->

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerViewGroupNames"
                android:layout_width="@dimen/_70sdp"
                android:layout_height="@dimen/_47sdp"
                android:layout_marginStart="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_15sdp"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/buttonLnq">

            </androidx.recyclerview.widget.RecyclerView>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewProfile"
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:padding="2dp"
                app:layout_constraintStart_toStartOf="@+id/textViewName"
                app:layout_constraintTop_toTopOf="@+id/textViewStatusMessage" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewFavoriteBorder"
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/textViewName"
                app:layout_constraintTop_toTopOf="@+id/textViewStatusMessage"
                app:srcCompat="@drawable/icon_fav_border" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewProfileLinked"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:background="@drawable/bg_circle_green"
                android:padding="@dimen/_3sdp"
                android:visibility="invisible"
                app:layout_constraintCircle="@+id/imageViewProfile"
                app:layout_constraintCircleAngle="45"
                app:layout_constraintCircleRadius="@dimen/_37sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/icon_link"
                app:tint="@color/colorBlackSettingNewTheme" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewQuoteLeft"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:visibility="invisible"
                app:layout_constraintRight_toLeftOf="@+id/textViewStatusMessage"
                app:layout_constraintStart_toEndOf="@+id/imageViewProfile"
                app:layout_constraintTop_toTopOf="@+id/textViewStatusMessage"
                app:srcCompat="@mipmap/left_qoute_new" />

            <TextView
                android:id="@+id/textViewStatusMessage"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_30sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:maxLines="3"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_16ssp"
                app:layout_constraintEnd_toStartOf="@+id/imageViewQuoteRight"
                app:layout_constraintStart_toEndOf="@+id/imageViewQuoteLeft"
                app:layout_constraintTop_toBottomOf="@+id/textViewconnections" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewQuoteRight"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/textViewStatusMessage"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/textViewStatusMessage"
                app:srcCompat="@mipmap/right_qoute_new" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewHomeLocation"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:visibility="invisible"
                app:layout_constraintLeft_toLeftOf="@+id/textViewStatusMessage"
                app:layout_constraintTop_toBottomOf="@+id/textViewStatusMessage"
                app:srcCompat="@drawable/icon_home"
                app:tint="@color/colorBlackSettingItemsNewTheme" />

            <TextView
                android:id="@+id/textViewHomeLocation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp"
                android:singleLine="true"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintBottom_toBottomOf="@+id/imageViewHomeLocation"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageViewHomeLocation"
                app:layout_constraintTop_toTopOf="@+id/imageViewHomeLocation" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewCurrentLocation"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_6sdp"
                android:visibility="invisible"
                app:layout_constraintLeft_toLeftOf="@+id/textViewStatusMessage"
                app:layout_constraintTop_toBottomOf="@+id/textViewHomeLocation"
                app:srcCompat="@drawable/icon_pin"
                app:tint="@color/colorBlackSettingItemsNewTheme" />

            <TextView
                android:id="@+id/textViewCurrentLocation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp"
                android:singleLine="true"
                android:textColor="@color/colorBlackHintNewTheme"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintBottom_toBottomOf="@+id/imageViewCurrentLocation"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageViewCurrentLocation"
                app:layout_constraintTop_toTopOf="@+id/imageViewCurrentLocation" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageViewLinkedConnection"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_9sdp"
                android:visibility="invisible"
                app:layout_constraintLeft_toLeftOf="@+id/textViewStatusMessage"
                app:layout_constraintTop_toBottomOf="@+id/imageViewCurrentLocation"
                app:srcCompat="@drawable/icon_link"
                app:tint="@color/colorBlackSettingItemsNewTheme" />

            <RelativeLayout
                android:id="@+id/linearLayoutConnectionInfo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageViewLinkedConnection"
                app:layout_constraintTop_toTopOf="@+id/imageViewLinkedConnection">

                <TextView
                    android:id="@+id/textViewConnectionSince"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:id="@+id/textViewConnectionDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3sdp"
                    android:layout_marginLeft="@dimen/_3sdp"
                    android:layout_toEndOf="@+id/textViewConnectionSince"
                    android:layout_toRightOf="@+id/textViewConnectionSince"
                    android:singleLine="true"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textViewConnectionIn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:layout_toEndOf="@+id/textViewConnectionDate"
                    android:layout_toRightOf="@+id/textViewConnectionDate"
                    android:singleLine="true"
                    android:text="@string/in"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_11ssp"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/textViewConnectionLocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textViewConnectionSince"
                    android:singleLine="true"
                    android:textColor="@color/colorBlackHintNewTheme"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/pagerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:clickable="true"
                android:focusable="true"
                android:visibility="invisible"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutConnectionInfo">

                <TextView
                    android:id="@+id/textViewNotes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25sdp"
                    android:layout_marginLeft="@dimen/_25sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:text="@string/notes"
                    android:textColor="@color/colorBlackSettingItemsNewTheme"
                    android:textSize="@dimen/_14ssp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/textViewAbout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:text="@string/about"
                    android:textColor="@color/colorBlackSettingItemsNewTheme"
                    android:textSize="@dimen/_14ssp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewNotes"
                    app:layout_constraintLeft_toRightOf="@+id/textViewNotes"
                    app:layout_constraintTop_toTopOf="@+id/textViewNotes" />

                <TextView
                    android:id="@+id/textViewHistory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:text="@string/history"
                    android:textColor="@color/colorBlackSettingItemsNewTheme"
                    android:textSize="@dimen/_14ssp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewAbout"
                    app:layout_constraintLeft_toRightOf="@+id/textViewAbout"
                    app:layout_constraintTop_toTopOf="@+id/textViewAbout" />

                <TextView
                    android:id="@+id/textViewContactList"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:text="@string/contactlist"
                    android:textColor="@color/colorBlackSettingItemsNewTheme"
                    android:textSize="@dimen/_14ssp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/textViewHistory"
                    app:layout_constraintLeft_toRightOf="@+id/textViewHistory"
                    app:layout_constraintTop_toTopOf="@+id/textViewHistory" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <lnq.com.lnq.custom.views.view_pagify.ViewPagify
                android:id="@+id/viewPagify"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/_20sdp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pagerView">

            </lnq.com.lnq.custom.views.view_pagify.ViewPagify>

